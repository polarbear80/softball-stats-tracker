                <TableRow android:id="@+id/tableRow5" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <TextView android:id="@+id/tvStrikeOuts" android:text="@string/strikeouts" android:layout_height="wrap_content" android:textAppearance="?android:attr/textAppearanceSmall" android:layout_marginLeft="20dip" android:layout_width="wrap_content"></TextView>

                    <Spinner
                	    android:id="@+id/spStrikeOuts"
                	    android:layout_width="50dp"
                	    android:layout_height="40dp"
                	    android:layout_marginLeft="20dip"
                	    android:layout_weight="1"
                	    android:background="@drawable/spinner" />
                    <TextView android:layout_height="wrap_content" android:textAppearance="?android:attr/textAppearanceSmall" android:layout_marginLeft="20dip" android:layout_width="wrap_content" android:text="@string/sacrifices" android:id="@+id/tvSacs"></TextView>

                    <Spinner
                	    android:id="@+id/spSacs"
                	    android:layout_width="50dp"
                	    android:layout_height="40dp"
                	    android:layout_marginLeft="20dip"
                	    android:layout_weight="1"
                	    android:background="@drawable/spinner" />
                </TableRow>
                <TableRow android:id="@+id/tableRow6" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <TextView android:id="@+id/tvROE" android:text="@string/reachonerror" android:layout_height="wrap_content" android:textAppearance="?android:attr/textAppearanceSmall" android:layout_marginLeft="20dip" android:layout_width="wrap_content"></TextView>

                    <Spinner
                	    android:id="@+id/spROE"
                	    android:layout_width="50dp"
