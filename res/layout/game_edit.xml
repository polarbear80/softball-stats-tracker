                	    android:background="@drawable/spinner" />
                    <TextView android:id="@+id/tvFC" android:text="@string/fielderschoice" android:layout_height="wrap_content" android:textAppearance="?android:attr/textAppearanceSmall" android:layout_marginLeft="20dip" android:layout_width="wrap_content"></TextView>

                    <Spinner
                	    android:id="@+id/spFC"
                	    android:layout_width="50dp"
                	    android:layout_height="40dp"
                	    android:layout_marginLeft="20dip"
                	    android:layout_weight="1"
                	    android:background="@drawable/spinner" />
                </TableRow>
             </TableLayout>
        </LinearLayout>
        <Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/save" android:id="@+id/bSave"></Button>
            <requestFocus></requestFocus>
        <ListView android:id="@android:id/list" android:layout_height="wrap_content" android:layout_width="match_parent"></ListView>
    
    </LinearLayout>
 </LinearLayout>
